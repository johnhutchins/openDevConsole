{
    "name": "Open Salesforce Develoer Console",
    "version": "2.1",
    "manifest_version": 2,
    "description": "Opens the Salesforce developer console from your currently opened Org",
    "background": {
        "persistent": false,
        "scripts": [
            "background_script.js"
        ]
    },
    "permissions": [
        "https://*.salesforce.com/",
        "activeTab",
        "tabs"
    ],
    "browser_action": {
        "default_icon": "img.png"
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "windows": "Ctrl+Shift+Y",
                "mac": "Command+Shift+Y",
                "chromeos": "Ctrl+Shift+Y",
                "linux": "Ctrl+Shift+Y"
            }
        }
    }
}