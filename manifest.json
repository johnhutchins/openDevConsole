{
    "name": "Open Dev Console",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Opens dev console with current url",
    "background": {
        "persistent": false,
        "scripts": [
            "background_script.js"
        ]
    },
    "permissions": [
        "https://*.salesforce.com/",
        "activeTab",
        "tabs"
    ],
    "browser_action": {
        "default_icon": "img.png"
    }
}